  keywords = {Computational Fluid Dynamics (CFD),Large Eddy Simulation (LES),Numerical Uncertainty,Quality Index},
  number = {5},
  pages = {949--958},
  title = {{Index of resolution quality for large eddy simulations}},
  volume = {127},
  year = {2005},
}


