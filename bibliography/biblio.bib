  month = {jul},
  pages = {354--364},
  publisher = {Elsevier},
  title = {{A multiple mapping conditioning mixing model with a mixture-fraction like reference variable. Part 2: RANS implementation and validation against a turbulent jet flame}},
  url = {https://www.sciencedirect.com/science/article/pii/S0010218017301098},
