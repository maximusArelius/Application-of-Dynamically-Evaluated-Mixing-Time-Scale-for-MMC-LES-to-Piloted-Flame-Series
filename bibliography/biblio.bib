  issn = {15731987},
  journal = {Flow, Turbulence and Combustion},
  keywords = {Evaporation,FGM,MILD combustion,Spray,Transported PDF},
  title = {{Transported PDF Modeling of Ethanol Spray in Hot-Diluted Coflow Flame}},
  year = {2016},
}


