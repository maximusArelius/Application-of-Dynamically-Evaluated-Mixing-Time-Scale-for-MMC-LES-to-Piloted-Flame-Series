  journal = {Combustion and Flame},
  keywords = {Combust ion noise,Hybrid DNS/APE-RF approach,Turbulent spray flame,Two-phase reacting flow},
  pages = {168--191},
  publisher = {Elsevier Inc.},
  title = {{Combustion noise analysis of a turbulent spray flame using a hybrid DNS/APE-RF approach}},
  url = {https://doi.org/10.1016/j.combustflame.2018.10.041},
  volume = {200},
