  number = {2},
  pages = {322--339},
  publisher = {Taylor {\&} Francis},
  title = {{Transported Joint Probability Density Function Simulation of Turbulent Spray Flames Combined with a Spray Flamelet Model Using a Transported Scalar Dissipation Rate}},
  url = {http://dx.doi.org/10.1080/00102202.2016.1214584 https://doi.org/10.1080/00102202.2016.1214584},
