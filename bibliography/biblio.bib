  issn = {03019322},
  journal = {International Journal of Multiphase Flow},
  keywords = {CFD,Flash-boiling,Gasoline direct injection,Homogeneous relaxation model (HRM),Liquid-gas interface,Sigma-Y,Spray plume angle},
  pages = {21--33},
