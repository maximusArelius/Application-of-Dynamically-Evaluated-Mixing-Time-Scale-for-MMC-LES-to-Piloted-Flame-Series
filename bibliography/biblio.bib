  isbn = {1540-7489},
  issn = {15407489},
  journal = {Proceedings of the Combustion Institute},
  keywords = {CMC modeling,Fundamental aspects,Lifted flames,Non-premixed,Turbulent combustion},
  pages = {1705--1712},
  title = {{CMC simulations of lifted turbulent jet flame in a vitiated coflow}},
  url = {www.elsevier.com/locate/proci},
  volume = {32 II},
  year = {2009},
}


