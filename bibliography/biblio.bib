@article{Straub2016,
  abstract = {A stochastic implementation of the Multiple Mapping Conditioning (MMC) model has been used for the modelling of turbulence-chemistry interactions in a series of turbulent jet diffusion flames with varying degree of local extinction (Sandia Flames D-F). The mapping function approximates the cumulative probability distribution of mixture fraction and the corresponding variance can be controlled by a standard implementation of the scalar mixing time scale. The conditional fluctua- tions are controlled by a minor dissipation time scale, $\tau$ min . The results show a clear dependence of the conditional fluctuations on the choice of the minor time scale, and the appropriate value for turbulent jet flames is similar to values determined in related DNS studies of homogeneous turbulent reacting flows. The predictions of means and variances of temperature and species mass fractions are very good for all flames indicating an appropriate modelling of the conditional vari- ances. Further sensitivity studies with respect to particle number density demonstrate a relative insensitivity of the results to the particle number in the numerical solution procedure. Good results can be obtained with as few as 10 particles per cell allowing for a computationally inexpensive implementation of a Monte-Carlo/PDF method.},
  author = {Straub, Carmen and De, Santanu and Kronenburg, Andreas and Vogiatzaki, Konstantina},
  doi = {10.1080/13647830.2016.1191677},
  file = {:home/eshan/Documents/Mendeley Desktop/St. - 2016 - The effect of timescale variation in multiple mapping conditioning mixing of PDF calculations for Sandia Flame series D – F: - 2016 - The effect of timescale variation in multiple mapping conditioning mixing of PDF calculations for Sandia Flame series D – F},
  issn = {17413559},
  journal = {Combustion Theory and Modelling},
  keywords = {PDF methods,RANS,Sandia Flame series (D–F),multiple mapping conditioning,turbulent combustion},
  number = {5},
