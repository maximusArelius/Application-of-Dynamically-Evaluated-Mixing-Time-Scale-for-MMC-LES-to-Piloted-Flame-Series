  isbn = {0010-2180},
  issn = {15562921},
  journal = {Combustion and Flame},
  keywords = {Atomization,Image processing,Spray combustion,Turbulence},
  number = {4},
  pages = {978--996},
  publisher = {The Combustion Institute},
  title = {{Characterization of atomization and combustion in moderately dense turbulent spray flames}},
