  issn = {00102180},
  journal = {Combustion and Flame},
  keywords = {Computational modeling,Laser spectroscopy,Multiscalar measurements,Reactions in flames,Vitiated flow},
  month = {dec},
  number = {4},
  pages = {491--506},
  publisher = {Elsevier},
  title = {{Lifted methane-air jet flames in a vitiated coflow}},
  url = {https://www.sciencedirect.com/science/article/pii/S0010218005002312},
