  isbn = {0010-2180},
  issn = {15562921},
  journal = {Combustion and Flame},
  keywords = {CARS,Hot-diluted coflow,Liquid breakup,MILD combustion,PDA,Spray flames},
  number = {3},
  pages = {759--773},
  publisher = {The Combustion Institute},
  title = {{Spray flame structure in conventional and hot-diluted combustion regime}},
  url = {http://dx.doi.org/10.1016/j.combustflame.2014.07.033},
  volume = {162},
