@article{Sirignano2006,
  abstract = {A forefront computational issue in the combustion of liquid fuels concerns the treatment of many vaporizing, burning fuel droplets. The analysis of many vaporizing liquid-fuel surfaces in close proximity to each other and enveloped by flames has recently been exam- ined.1–3 A brief review will be given here. New methods allow the treatment of complex quasi-steady spray-combustion configurations with greater ease than previously possible. Examples of configurations include hundreds of fuel droplets, liquid-fuel wall films and pools, and liquid-fuel free films. Analyses are presented for liquid-fuel vaporization and burning both with unitary and non-unitary Lewis number (i.e., similar and non-similar heat and mass diffusion) in a general geometrical situation, e.g., a dense spray. Variable transport properties are consid- ered and only Stefan flow is allowed. It is shown that the problem can conveniently be separated, using a mass-flux potential function (dependent only on the geometry), into a one-dimensional problem for the quasi-steady, gas-phase scalar properties and a three- dimensional problem for the mass-flux potential which satisfies Laplace's equation. The scalar properties are shown to be universal (i.e., independent of configuration) functions of the mass-flux potential only. It is demonstrated that a mass-flux-weighted sensible spe- cific enthalpy is more natural and convenient than the traditional mass-weighted value for the non-unitary-Lewis-number case. This modification results in a new definition of the Lewis number. A generalization of the classical Spalding4,5 heat transfer number is presented. The theory predicts scalar gas-phase profiles, flame position, and vaporization rates. Quantitative results are presented for special cases where the Lewis number is piece- wise constant. The thin-flame temperature and the effective latent heat of vaporization can be determined as functions of the liquid-surface temperature via solution of nonlinear algebraic equations; these values do not depend on the specific configuration and therefore have some universality.},
  author = {Sirignano, William},
  doi = {10.2514/6.2006-1522},
  file = {:home/eshan/Documents/Mendeley Desktop/Sirignano - 2006 - Recent Advances in Spray Combustion Theory - American Institute of Aeronautics and Astronautics.pdf:pdf},
  journal = {American Institute of Aeronautics and Astronautics},
  keywords = {spray combustion},
  mendeley-tags = {spray combustion},
  number = {January},
  title = {{Recent Advances in Spray Combustion Theory}},
  year = {2006},
}


