  issn = {03019322},
  journal = {International Journal of Multiphase Flow},
  number = {3},
  pages = {247--260},
  publisher = {Elsevier Ltd},
  title = {{Numerical simulation of primary break-up and atomization: DNS and modelling study}},
  url = {http://dx.doi.org/10.1016/j.ijmultiphaseflow.2008.11.005},
