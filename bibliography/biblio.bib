  file = {:home/eshan/Documents/Mendeley Desktop/Thomas et al. - 2017 - Femtosecond Chirped-Probe-Pulse Coherent Anti-Stokes Raman Scattering Thermometry of Nitrogen in a Piloted Spray.pdf:pdf},
  isbn = {978-1-62410-447-3},
  journal = {American Institute of Aeronautics and Astronautics},
  keywords = {cars,femtosecond,spectroscopy,spray flames},
  number = {January},
  pages = {1--8},
  title = {{Femtosecond Chirped-Probe-Pulse Coherent Anti-Stokes Raman Scattering Thermometry of Nitrogen in a Piloted Spray Burner}},
  url = {http://arc.aiaa.org/doi/10.2514/6.2017-0032},
