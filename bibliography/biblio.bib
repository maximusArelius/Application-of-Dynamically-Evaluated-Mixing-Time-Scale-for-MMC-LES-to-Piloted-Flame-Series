  abstract = {A stochastic sub-grid model is often used to accurately represent particle dispersion in turbulent flows using large eddy simulations. Models of this type have a free parameter, the dispersion coefficient, which is not universal and is strongly grid-dependent. In the present paper, a dynamic model for the evaluation of the coefficient is proposed and validated in decaying homogeneous isotropic turbulence. The grid dependence of the static coefficient is investigated in a turbulent mixing layer and compared to the dynamic model. The dynamic model accurately predicts dispersion statistics and resolves the grid-dependence. Dispersion statistics of the dynamically calculated constant are more accurate than any static coefficient choice for a number of grid spacings. Furthermore, the dynamic model produces less numerical artefacts than a static model and exhibits smaller sensitivity in the results predicted for different particle relaxation times. {\textcopyright} 2013 AIP Publishing LLC.},
  author = {Pesmazoglou, I. and Kempf, A. M. and Navarro-Martinez, S.},
  doi = {10.1063/1.4848855},
  file = {:home/eshan/Documents/Mendeley Desktop/Pesmazoglou, Kempf, Navarro-Martinez - 2013 - A dynamic model for the lagrangian stochastic dispersion coefficient - Physics of Fluids.pdf:pdf},
  issn = {10706631},
  journal = {Physics of Fluids},
  number = {12},
  title = {{A dynamic model for the lagrangian stochastic dispersion coefficient}},
  volume = {25},
  year = {2013},
}


@inproceedings{Stein2005,
  abstract = {The non-premixed turbulent Sydney Swirl Burner is a target of the workshop-series on turbu- lent non-premixed flames (TNF). Thorough experimental investigations in Sydney and Sandia provided a vast set of experimental data for ten different configurations. This flame series al- lows the examination of the effect of various parameters like fuel composition, flow rates and swirl number. The large eddy simulation of the entire flame series promises detailed insight into turbulent non-premixed swirl combustion, worthwhile the expensive simulations. To im- prove the reliability of these simulations and to reduce its cost, elaborate preliminary studies must be carried out, which are detailed in this paper. The spatial resolution requirements of the mesh are discussed in terms of the resolved kinetic energy. The time required for the sim- ulation to achieve a fully developed flow is determined, as well as an appropriate sampling period for statistics. The immersed boundary condition technique applied to these flames is explained and its short-comings are discussed. The paper shows first and second moments for the velocity components in the non-reactive flow and preliminary mixture fraction results for one flame of the series. Results are in reasonable agreement with experimental data, while some deficiencies show that further grid refinement and more accurate inflow data are needed before the simulation of the entire flame series can be attempted.},
  author = {Stein, Oliver and Kempf, Andreas M. and Janicka, Johannes},
  booktitle = {MCS4},
