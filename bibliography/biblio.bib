  issn = {15562921},
  journal = {Combustion and Flame},
  keywords = {Dimethyl ether,LES,MMC,Turbulent combustion modelling},
  number = {x},
  pages = {110--121},
  publisher = {Elsevier Inc.},
  title = {{Sparse-Lagrangian MMC modelling of the Sandia DME flame series}},
